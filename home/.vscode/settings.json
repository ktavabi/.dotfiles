{
    "[jsonc]": {
        "editor.defaultFormatter": "trunk.io"
    },
    "[python]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "editor.accessibilitySupport": "off",
    "editor.formatOnSave": true,
    "editor.tabCompletion": "on",
    "explorer.openEditors.sortOrder": "fullPath",
    "files.exclude": {
        "**/.trunk/*out": true
    },
    "files.watcherExclude": {
        "**/.trunk/*out": true
    },
    "git.allowNoVerifyCommit": true,
    "html.completion.attributeDefaultValue": "singlequotes",
    "html.format.endWithNewline": true,
    "json.schemas": [],
    "jupyter.generateSVGPlots": true,
    "jupyter.pylanceHandlesNotebooks": true,
    "jupyter.sendSelectionToInteractiveWindow": true,
    "notebook.consolidatedRunButton": true,
    "pylance.insidersChannel": "daily",
    "python.formatting.blackArgs": [
        "--line-length=99"
    ],
    "python.formatting.provider": "black",
    "python.sortImports.args": [
        "--profile",
        "black"
    ],
    "source.organizeImports": true,
    "telemetry.telemetryLevel": "crash",
    "terminal.external.osxExec": "iTerm.app",
    "trunk.trunkGrayOutNonBlockingIssues": false,
    "git.autofetch": true,
    "workbench.colorTheme": "Dark SynthWave '84",
    "workbench.iconTheme": "Monokai Pro Icons",
    "sync.autoUpload": true,
    "sync.quietSync": true
}
